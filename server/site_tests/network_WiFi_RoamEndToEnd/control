AUTHOR = 'bmahadev, harpreet, krisr'
TIME = 'SHORT'
NAME = 'network_WiFi_RoamEndToEnd'
TEST_TYPE = 'Server'
ATTRIBUTES = "suite:wifi_matfunc, suite:wifi_matfunc_bcm4356, suite:wifi_matfunc_intel7265, suite:wifi_matfunc_marvell8897, suite:wifi_release"
SUITE = ('wifi_matfunc, wifi_matfunc_bcm4356, wifi_matfunc_marvell8897,'
         'wifi_matfunc_intel7265, wifi_release')
DEPENDENCIES = 'wificell'

DOC = """
WiFi_RoamEndToEnd test configures two APs with the same ssid and runs the
client side network_RoamWifiEndToEnd test which uses these APs to test
WiFi roaming between two Open Networks.
"""

def run(machine):
    host = hosts.create_host(machine)
    job.run_test('network_WiFi_RoamEndToEnd',
                 host=host,
                 raw_cmdline_args=args)


parallel_simple(run, machines)

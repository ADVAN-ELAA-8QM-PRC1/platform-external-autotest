# Purpose: This test case verifies that the DUT accepts ascii and non-ascii
# type characters as the SSID

{ "name":"NonAscii_SSID",
  "steps":[
    [ "create",         { "type":"hostap" } ],

    [ "config",         { "channel":"2412", "mode":"11g" }],

    # Ascii characters from 0x20 to 0xff skipping the following characters
    # '\x21' ('!') character because bash had parsing issues with ssh command
    # '\x60' ('`') character because bash also had parsing issues
    # '\x7f' because that's the Del character
    [ "config",         { "ssid" : "\x20\x22\x23\x24\x25\x26\x27\x28\x29\x2a"
                                   "\x2b\x2c\x2d\x2e\x2f\x30\x31\x32\x33\x34"
                                   "\x35\x36\x37\x38\x39\x3a\x3b\x3c\x3d\x3e"
                                   "\x3f" } ],
    # backslash ('\x5c') is needed to escape the trailing double quote ('\x22')
    # so that the full ssid value is parsed correctly in WiFiTest.connect()
    [ "connect",        { "ssid" : "\x20\x5c\x22\x23\x24\x25\x26\x27\x28\x29"
                                   "\x2a\x2b\x2c\x2d\x2e\x2f\x30\x31\x32\x33"
                                   "\x34\x35\x36\x37\x38\x39\x3a\x3b\x3c\x3d"
                                   "\x3e\x3f",
                          "security":"none" } ],
    [ "client_ping",    { "count": "10" } ],
    [ "disconnect" ],
    [ "deconfig" ],


    [ "config",         { "ssid" : "\x40\x41\x42\x43\x44\x45\x46\x47\x48\x49"
                                   "\x4a\x4b\x4c\x4d\x4e\x4f\x50\x51\x52\x53"
                                   "\x54\x55\x56\x57\x58\x59\x5a\x5b\x5c\x5d"
                                   "\x5e\x5f" } ],
    [ "connect",        { "ssid" : "\x40\x41\x42\x43\x44\x45\x46\x47\x48\x49"
                                   "\x4a\x4b\x4c\x4d\x4e\x4f\x50\x51\x52\x53"
                                   "\x54\x55\x56\x57\x58\x59\x5a\x5b\x5c\x5d"
                                   "\x5e\x5f",
                          "security":"none" } ],
    [ "client_ping",    { "count":"10" } ],
    [ "disconnect" ],
    [ "deconfig" ],


    [ "config",         { "ssid" : "\x61\x62\x63\x64\x65\x66\x67\x68\x69\x6a"
                                   "\x6b\x6c\x6d\x6e\x6f\x70\x71\x72\x73\x74"
                                   "\x75\x76\x77\x78\x79\x7a\x7b\x7c\x7d\x7e",
                                   } ],
    [ "connect",        { "ssid" : "\x61\x62\x63\x64\x65\x66\x67\x68\x69\x6a"
                                   "\x6b\x6c\x6d\x6e\x6f\x70\x71\x72\x73\x74"
                                   "\x75\x76\x77\x78\x79\x7a\x7b\x7c\x7d\x7e",
                          "security":"none" } ],
    [ "client_ping",    { "count":"10" } ],
    [ "disconnect" ],
    [ "deconfig" ],

    [ "config",         { "ssid" : "\x80\x81\x82\x83\x84\x85\x86\x87\x88\x89"
                                   "\x8a\x8b\x8c\x8d\x8e\x8f\x90\x91\x92\x93"
                                   "\x94\x95\x96\x97\x98\x99\x9a\x9b\x9c\x9d"
                                   "\x9e\x9f" } ],
    [ "connect",        { "ssid" : "\x80\x81\x82\x83\x84\x85\x86\x87\x88\x89"
                                   "\x8a\x8b\x8c\x8d\x8e\x8f\x90\x91\x92\x93"
                                   "\x94\x95\x96\x97\x98\x99\x9a\x9b\x9c\x9d"
                                   "\x9e\x9f",
                          "security":"none" } ],
    [ "client_ping",    { "count":"10" } ],
    [ "disconnect" ],
    [ "deconfig" ],


    [ "config",         { "ssid" : "\xa0\xa1\xa2\xa3\xa4\xa5\xa6\xa7\xa8\xa9"
                                   "\xaa\xab\xac\xad\xae\xaf\xb0\xb1\xb2\xb3"
                                   "\xb4\xb5\xb6\xb7\xb8\xb9\xba\xbb\xbc\xbd"
                                   "\xbe\xbf" } ],
    [ "connect",        { "ssid" : "\xa0\xa1\xa2\xa3\xa4\xa5\xa6\xa7\xa8\xa9"

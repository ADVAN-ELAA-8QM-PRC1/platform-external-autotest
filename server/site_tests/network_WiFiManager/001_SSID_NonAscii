                                   "\xb4\xb5\xb6\xb7\xb8\xb9\xba\xbb\xbc\xbd"
                                   "\xbe\xbf",
                          "security":"none" } ],
    [ "client_ping",    { "count":"10" } ],
    [ "disconnect" ],
    [ "deconfig" ],


    [ "config",         { "ssid" : "\xc0\xc1\xc2\xc3\xc4\xc5\xc6\xc7\xc8\xc9"
                                   "\xca\xcb\xcc\xcd\xce\xcf\xd0\xd1\xd2\xd3"
                                   "\xd4\xd5\xd6\xd7\xd8\xd9\xda\xdb\xdc\xdd"
                                   "\xde\xdf" } ],
    [ "connect",        { "ssid" : "\xc0\xc1\xc2\xc3\xc4\xc5\xc6\xc7\xc8\xc9"
                                   "\xca\xcb\xcc\xcd\xce\xcf\xd0\xd1\xd2\xd3"
                                   "\xd4\xd5\xd6\xd7\xd8\xd9\xda\xdb\xdc\xdd"
                                   "\xde\xdf",
                          "security":"none" } ],
    [ "client_ping",    { "count":"10" } ],
    [ "disconnect" ],
    [ "deconfig" ],

    [ "config",         { "ssid" : "\xe0\xe1\xe2\xe3\xe4\xe5\xe6\xe7\xe8\xe9"
                                   "\xea\xeb\xec\xed\xee\xef\xf0\xf1\xf2\xf3"
                                   "\xf4\xf5\xf6\xf7\xf8\xf9\xfa\xfb\xfc\xfd"
                                   "\xfe\xff" } ],
    [ "connect",        { "ssid" : "\xe0\xe1\xe2\xe3\xe4\xe5\xe6\xe7\xe8\xe9"
                                   "\xea\xeb\xec\xed\xee\xef\xf0\xf1\xf2\xf3"
                                   "\xf4\xf5\xf6\xf7\xf8\xf9\xfa\xfb\xfc\xfd"
                                   "\xfe\xff",
                          "security":"none" } ],
    [ "client_ping",    { "count":"10" } ],
    [ "disconnect" ],
    [ "deconfig" ],

    # Using valid Unicode characters only as SSID
    [ "config",         { "ssid" : "中国"} ],
    [ "connect",        { "ssid" : "中国", "security":"none" } ],
    [ "client_ping",    { "count":"10" } ],
    [ "disconnect" ],
    [ "deconfig" ],

    # Using a single extended ASCII character (a-grave) as SSID
    [ "config",         { "ssid" : "\xe0"} ],
    [ "connect",        { "ssid" : "\xe0", "security":"none" } ],
    [ "client_ping",    { "count":"10" } ],
    [ "disconnect" ],
    [ "deconfig" ],

    # Using mix of ASCII and Unicode characters as SSID
    [ "config",         { "ssid" : "Chrome笔记本\xff"} ],
    [ "connect",        { "ssid" : "Chrome笔记本\xff", "security":"none" } ],
    [ "client_ping",    { "count":"10" } ],
    [ "disconnect" ],
    [ "deconfig" ],

    [ "destroy" ],
  ],
}

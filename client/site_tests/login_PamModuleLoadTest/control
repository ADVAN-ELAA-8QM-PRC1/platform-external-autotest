AUTHOR = "ChromeOS Team"
NAME = "login_PamMOduleLoadTest"
PURPOSE = "Verifies pam_google.so can be dynamically loaded."
CRITERIA = """
Fails under any of the following conditions:
  - pam_loader_ia32 cannot be opened
  - pam_sm_authenticate is not found
  - pam_loader returns an error
"""
TIME = "SHORT"
TEST_CATEGORY = "Functional"
TEST_CLASS = "login"
TEST_TYPE = "client"

DOC = """
  Tests that pam_google.so, the Linux-PAM module that
  implements authentication with a Google account, exists and can be
  dynamically loaded properly.
"""

job.run_test('login_PamModuleLoadTest')

;
; 1MB random with verify and mix read write.
;
[1MB_stress]
readwrite=randrw
rwmixread=15
bs=1m
ioengine=libaio
iodepth=32
bwavgtime=5000
iopsavgtime=5000
direct=1
thread=1
numjobs=8
do_verify=1
verify=meta
verify_interval=64k
verify_dump=1

filename=${FILENAME}
size=${FILESIZE}

; write partition/file twice
loops=2
